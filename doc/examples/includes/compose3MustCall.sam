mustCall(<delegate>, "NotLogged", "Delegate.test:file.read", <logged>, "Logger.$method").
mustCall(<logged>, "Logged", "Logger.$method:value=myUnderlying.$method", <readOnly>, "ReadOnly.read").
mustCall(<readOnly>, "Logged", "ReadOnly.read:value=myUnderlying.read", <file>, "File.read").

mayCall(<_testDriver>, ?Target, ?Method) :- isRef(?Target), hasMethod(?Type, ?Method).
mayCall(<_testDriver>, ?Target, ?Method) :- isRef(?Target), hasConstructor(?Type, ?Method).

checkCalls(<delegate>).
checkCalls(<file>).
checkCalls(<logged>).
checkCalls(<readOnly>).
